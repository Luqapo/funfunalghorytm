{ "env": {
  "browser": true,
  "es6": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "keyword-spacing": ["error", {
      "overrides": {
        "if": { "after": false },
        "while": { "after": false },
        "for": { "after": false },
        "switch": { "after": false },
        "catch": { "after": false }
      }
    }],
    "no-plusplus": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-unused-vars": "warn"
  }
}